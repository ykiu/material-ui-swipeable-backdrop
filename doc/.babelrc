{
  "presets": [
    ["@babel/preset-env", { "loose": true, "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": [
    ["macros"],
    ["@babel/plugin-transform-runtime"],
    ["@babel/proposal-object-rest-spread", { "loose": true }]
  ],
  "env": {
    "test": {
      "plugins": [
        "@babel/transform-modules-commonjs"
      ]
    }
  }
}
